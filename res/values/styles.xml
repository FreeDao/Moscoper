<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">

        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>

    <style name="NotifyCountText">
        <item name="android:textSize">@dimen/notify_count_size</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/icon_number</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingRight">2.0dip</item>
        <item name="android:visibility">invisible</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">-15.0dip</item>
        <item name="android:minWidth">20.0dip</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="DialogOk">
        <item name="android:textSize">@dimen/dialog_btn</item>
        <item name="android:textColor">@color/dialog_btn_ok</item>
    </style>

    <style name="DialogCancel">
        <item name="android:textSize">@dimen/dialog_btn</item>
        <item name="android:textColor">@color/dialog_btn_cancel</item>
    </style>

    <style name="Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="DialogText_Title">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="DialogText_Tips">
        <item name="android:textSize">12.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff969696</item>
    </style>

    <style name="DialogText_Message">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">#ff7d8184</item>
        <item name="android:gravity">left|center</item>
        <item name="android:singleLine">false</item>
        <item name="android:lineSpacingMultiplier">1.4</item>
    </style>

    <style name="DialogText_Input">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">#ff7d8184</item>
        <item name="android:gravity">left|center</item>
        <item name="android:singleLine">true</item>
        <item name="android:lineSpacingMultiplier">1.4</item>
    </style>

    <style name="Form">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/white_fa</item>
        <item name="android:paddingTop">1.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="qz_alertdialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:background">#00000000</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="action_sheet_layout_style">
        <item name="android:background">@drawable/action_sheet_base_9</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="action_sheet_title_layout_style">
        <item name="android:paddingLeft">@dimen/common_menu_dialog_tilte_paddingLR</item>
        <item name="android:paddingRight">@dimen/common_menu_dialog_tilte_paddingLR</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">15.0dip</item>
        <item name="android:layout_marginBottom">5.0dip</item>
    </style>

    <style name="action_sheet_content_style">
        <item name="android:paddingLeft">@dimen/common_menu_dialog_layout_paddingLR</item>
        <item name="android:paddingRight">@dimen/common_menu_dialog_layout_paddingLR</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
    </style>

    <style name="action_sheet_title_style">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@color/skin_action_sheet_title</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="action_sheet_secondary_title_style">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/skin_action_sheet_title</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">5.0dip</item>
    </style>

    <style name="action_sheet_btn_style">
        <item name="android:textSize">@dimen/textSizeS2</item>
        <item name="android:textColor">#ff333333</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">48.0dip</item>
    </style>

    <style name="action_sheet_white_btn_style" parent="@style/action_sheet_btn_style">
        <item name="android:background">@drawable/common_btn_gray_actionsheet</item>
    </style>

    <style name="action_sheet_green_btn_style" parent="@style/action_sheet_btn_style">
        <item name="android:textColor">@color/skin_color_button</item>
        <item name="android:background">@drawable/common_btn_green_actionsheet</item>
    </style>

    <style name="action_sheet_gray_btn_style" parent="@style/action_sheet_btn_style">
        <item name="android:textColor">@color/skin_color_button</item>
        <item name="android:background">@drawable/common_btn_black_actionsheet</item>
    </style>

    <style name="action_sheet_red_btn_style" parent="@style/action_sheet_btn_style">
        <item name="android:textColor">@color/skin_color_button</item>
        <item name="android:background">@drawable/common_btn_red_actionsheet</item>
    </style>

    <style name="action_sheet_cancel_btn_style" parent="@style/action_sheet_btn_style">
        <item name="android:textSize">@dimen/textSizeS2</item>
        <item name="android:textColor">@color/skin_color_button</item>
        <item name="android:background">@drawable/common_btn_black_actionsheet</item>
        <item name="android:text">@string/cancel</item>
    </style>

    <style name="mystyle" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/dialog_enter</item>
        <item name="@android:windowExitAnimation">@anim/dialog_exit</item>
    </style>

    <style name="UserStatistic">
        <item name="android:textSize">@dimen/user_statistic_size</item>
        <item name="android:textColor">@color/white_f1</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">@dimen/user_statistic_drawable_padding</item>
    </style>

    <style name="prod_detail_fav_btn_style" parent="@android:style/Widget.TextView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/white_ee</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">2.0dip</item>
        <item name="android:drawableLeft">@drawable/icon_profile_add_small</item>
        <item name="android:background">@drawable/near_by_red_btn</item>
        <item name="android:text">@string/prod_detail_btn_fav</item>
        <item name="android:paddingLeft">12dip</item>
        <item name="android:paddingRight">12dip</item>
        <item name="android:gravity">center_vertical|left</item>
    </style>

    <style name="prod_detail_cancel_fav_btn_style" parent="@android:style/Widget.TextView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/white_ee</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">2.0dip</item>
        <item name="android:drawableLeft">@drawable/icon_profile_subtract_small</item>
        <item name="android:background">@drawable/near_by_btn</item>
        <item name="android:text">@string/prod_detail_btn_fav</item>
        <item name="android:paddingLeft">12dip</item>
        <item name="android:paddingRight">12dip</item>
        <item name="android:gravity">center_vertical|left</item>
    </style>

    <style name="prod_detail_change_logo_btn_style" parent="@android:style/Widget.TextView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/white_ee</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">0.0dip</item>
        <item name="android:drawableLeft">@null</item>
        <item name="android:background">@drawable/near_by_red_btn</item>
        <item name="android:text">@string/prod_detail_btn_change_logo</item>
        <item name="android:paddingLeft">12dip</item>
        <item name="android:paddingRight">12dip</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="commentRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/comment_rating_bar_selector</item>
        <item name="android:minHeight">24dip</item>
        <item name="android:maxHeight">24dip</item>
    </style>

</resources>